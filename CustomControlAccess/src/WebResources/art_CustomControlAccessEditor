<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CustomControlAccessEditor</title>
    <style>
        body {
            background-color: aquamarine;
        }
    </style>
</head>

<body onload="registerFormContextOnload()">

</body>
<script>
    function registerFormContextOnload() {
        Object.defineProperty(window, "formContext", {
            set: function (val) {
                window.getFormContext = () => val;
                init();
            }
        })
    }
    function init() {
        console.log("window");
        console.log(window);
        console.log("window.formContext");
        console.log(window.formContext);
        console.log("formContext");
        console.log(formContext);
        console.log("window.getFormContext");
        console.log(window.getFormContext);
        console.log("window.getFormContext()");
        console.log(window.getFormContext());
    }
</script>

</html>